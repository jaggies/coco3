SubDir TOP apps mandel ;
SubDirHdrs $(TOP) lib os ;
SubDirHdrs $(TOP) lib coco ;
SubDirHdrs $(TOP) lib math ;
SubDirHdrs $(TOP) lib ray ;

if $(TARGET) = "coco3" {
	MANDEL = MANDEL.BIN ;
	CoCoDisk mandel.dsk : $(MANDEL) ;
} else {
    MANDEL = mandel ;
}

Depends $(MANDEL) : libos.a libcc3.a ;

LINKLIBS on $(MANDEL) = -los -lcc3 ;

Main $(MANDEL) :
	mandel.c
	;

