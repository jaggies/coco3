SubDir TOP apps intray ;
SubDirHdrs $(TOP) lib os ;
SubDirHdrs $(TOP) lib math ;
SubDirHdrs $(TOP) lib coco ;
SubDirHdrs $(TOP) lib ray ;

if $(TARGET) = coco3 {
	INTRAY = INTRAY.BIN ;
	CoCoDisk intray.dsk : $(INTRAY) ;
} else {
	INTRAY = intray ;
}

Depends $(INTRAY) : libray.a ;

LINKLIBS on $(INTRAY) = -lray -los -lcc3 ;

Main $(INTRAY) :
	intray.c
	;
