SubDir TOP apps intray ;
SubDirHdrs $(TOP) lib os ;
SubDirHdrs $(TOP) lib coco ;

Main INTRAY.BIN :
	intray.c
	dither.c
	fixed.c
	icamera.c
	ivec2i.c
	ivec3i.c
	imath.c
	isphere.c
	;

CoCoDisk intray.dsk : INTRAY.BIN ;

if $TARGET = coco3 {
	Main COCORAY.BIN :
		cocoray.c
		dither.c
		fixed.c
		icamera.c
		ivec2i.c
		ivec3i.c
		imath.c
		isphere.c
		;
	CoCoDisk cocoray.dsk : COCORAY.BIN ;
}



