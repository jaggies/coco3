SubDir TOP apps test ;
SubDirHdrs $(TOP) lib os ;

if $(TARGET) = "coco3" {
	ObjectCcFlags main.o : -DCOCO3 ;
	PRINTF = PRINTF.BIN ;
	FLOAT = FLOAT.BIN ;
	CoCoDisk float.dsk : $(FLOAT) ;
	CoCoDisk printf.dsk : $(PRINTF) ;
} else {
	PRINTF = printf ;
	FLOAT = float ;
	Main memset24 : memset24.c ;
}

LINKLIBS on $(FLOAT) = -los ;

Main $(PRINTF) : printf.c ;
Main $(FLOAT) : float.c ;


